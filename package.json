{
  "name": "n8n-nodes-wa-akg",
  "version": "1.0.7",
  "description": "n8n node for WA-AKG WhatsApp Gateway",
  "keywords": [
    "n8ncommunity-node",
    "whatsapp",
    "wa-akg"
  ],
  "license": "MIT",
  "homepage": "https://wa-akg.aikeigroup.net",
  "author": {
    "name": "WA-AKG",
    "email": "contact@aikeigroup.net"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WaAkgApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WaAkg/WaAkg.node.js",
      "dist/nodes/WaAkgTrigger/WaAkgTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "prettier": "^2.7.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "n8n-core": "^1.122.16",
    "n8n-workflow": "^1.120.9"
  }
}